					<hr/>
				</div>
				<nav class="col-sm-12 col-lg-12">
					<div class="col-sm-12 col-lg-3">
						<a href="/about/">About</a>
					</div>
					<div class="col-sm-12 col-lg-3">					
						<a href="/">Portfolio</a>
					</div>
					<div class="col-sm-12 col-lg-3">					
						<a href="/contact/">Contact</a>
					</div>
				</nav>
				<div class="col-sm-12 col-lg-12">
					<hr/>
				</div>
		</div>
	</header>
<!---End Header--->
<!---Start Content --->
	<main>
			<%= yield %>
	</main>
<!--- End Content --->	
